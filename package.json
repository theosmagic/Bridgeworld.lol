    "motion-plus": "^1.4.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router": "^7.9.3",
    "tailwind-merge": "^3.3.1",
    "viem": "^2.33.3",
    "wagmi": "^2.17.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.1",
    "@cloudflare/vite-plugin": "^1.13.8",
